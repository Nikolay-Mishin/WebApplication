{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		//"ordered-imports": false
		"ordered-imports": [true, {
			"import-sources-order": "any",
			"named-imports-order": "any",
			"grouped-imports": false,
			"module-source-path": "basename"
		}]
	},
	"jsRules": {},
	"rulesDirectory": []
}
